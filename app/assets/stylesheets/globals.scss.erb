$primary-blue: #007bc4;
$primary-green: #69A30D;
$primay-active-button-text-color: #ffffff;
$primary-bold: 600;

.module, .modal {
  .selectric-labeled {
    display: table;
    width: 100%;
    &>* {
      display:table-cell;
      width:100%;
      text-align: center;
      vertical-align:middle;
    }
    span {
      padding-left: 10px;
      width:100%;
      max-width: 25%;
      display: inline-block;
      font-size: 11px;
      color: rgb(131, 135, 128);
      text-transform: uppercase;
      text-align: left;
    }
    .selectric-wrapper {
      width:100%;
      max-width: 75%;
      display: inline-block;
    }
  }
}
